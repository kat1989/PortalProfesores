@model List<DataAccess.Model.OfertaAcademica>
@{Layout = null;}

<head>
    <title>Consulta de Oferta de Asignaturas</title>
    <meta http-equiv="Content-Language" content="es-do">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/estilos.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jscripts_app.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Events.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")" type="text/javascript"></script>
    <script language="javascript">
        var _siteUrl = "http://www.intec.edu.do/index.html?"
</script>
    <script language="javascript" src="/appintec.net/include/jscripts_app.js"></script>
    <script language="JavaScript" type="text/javascript" src="/siteintec.net/include/moz1.js"></script>
    <!--[if IE]>
<link rel="stylesheet" type="text/css" href="/appintec.net/include/estilosIE.css" >
<![endif]-->
</head>
<body>
    <center>
        <div id="divConsDos" align="center">
            <table class="MantForm" style="width: 1350px" border="0" cellpadding="0" cellspacing="0" style="padding-bottom:10px;">
                <tbody>
                    <tr class="OpcTopMenu">
                        <td colspan="2">
                            <table border="0" cellpadding="0" cellspacing="0" height="10" width="100%">
                                <tbody>
                                    <tr>
                                        <td class="OpcTopMenu" nowrap="nowrap" valign="center">
                                            Consulta de Oferta de Asignaturas
                                        </td>
                                        <td>
                                        </td>
                                        <td align="right" valign="center">
                                            <img style="cursor: hand" onclick="javascript:ScrAyuda('HelpOfertaTrimestral')" alt="Ayuda/Información de la Página"
                                                src="/appintec.net/images/help.gif" border="0" height="16">
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    <tr class="ColDetail">
                        <td style="border-right: #ffffff 1px solid; border-bottom: #d4d4d4 1px solid" nowrap="nowrap"
                            valign="top">
                            <br>
                        </td>
                        <td align="center">
                            <!-- Generado -->
                            <table>
                                <tbody>
                                    <tr>
                                        <td nowrap="nowrap">
                                            &nbsp;&nbsp;<strong><span id="lblArea">@Model.First().Area</span></strong>&nbsp;&nbsp;
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <!-- Final Generado -->
                        </td>
                    </tr>
                </tbody>
            </table>        
        
        <table class="MantForm" style="width: 1350px" border="0" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <td class="OpcTopMenu" colspan="2" align="center" nowrap="nowrap">
                        Resultado de la Búsqueda
                    </td>
                </tr>
                <tr>
                    <td align="center">
                        <table rules="all" id="grdBuscar" border="1" cellpadding="2" cellspacing="1">
                            <tbody>
                                <tr class="DataGridHeader" align="Center">
                                    <td>
                                        <font color="White" face="Verdana"><b>Código</b></font>
                                    </td>
                                    <td width="400px">
                                        <font color="White" face="Verdana"><b>Asignatura</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Sección</b></font>
                                    </td>
                                    <td width="50px">
                                        <font color="White" face="Verdana"><b>Aula</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Lunes</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Martes</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Miércoles</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Jueves</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Viernes</b></font>
                                    </td>
                                    <td>
                                        <font color="White" face="Verdana"><b>Sábado</b></font>
                                    </td>
                                    <td width="400px">
                                        <font color="White" face="Verdana"><b>Profesor</b></font>
                                    </td>
                                </tr>
                                @foreach (var item in Model)
                                {
                                <tr bgcolor="White">
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Codigo</font>
                                    </td>
                                    <td width="400px" style="text-align: left;">
                                        <font color="Black" face="Verdana">@item.Asignatura</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Seccion</font>
                                    </td>
                                    <td style="text-align: center;" width="50px">
                                        <font color="Black" face="Verdana">@item.Aula</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Horario.Lunes</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Horario.Martes</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Horario.Miercoles</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Horario.Jueves</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Horario.Viernes</font>
                                    </td>
                                    <td style="text-align: center;">
                                        <font color="Black" face="Verdana">@item.Horario.Sabado</font>
                                    </td>
                                    <td width="400px">
                                        <font color="Black" face="Verdana">@item.Profesor</font>
                                    </td>
                                </tr>
                                }
                            </tbody>
                        </table>
                    </td>
                </tr>
            </tbody>
        </table>
        </div>
    </center>
    </form>
    <div id="msgError">
        <font color="red"></font>
    </div>
</body>
</html>